<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <TargetFramework>net8.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <Nullable>enable</Nullable>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>  
  </PropertyGroup>
  
  <ItemGroup>
    <Content Include="..\DreamineApp.Resources\Resources\icon.png">
      <Link>Resources\icon.png</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  
  <ItemGroup>
    <PackageReference Include="Dreamine.MVVM.Behaviors.Wpf" Version="1.0.2" />
    <PackageReference Include="Dreamine.MVVM.Locators.Wpf" Version="1.0.3" />
    <ProjectReference Include="..\DreamineApp.Resources\DreamineApp.Resources.csproj" />
  </ItemGroup>

</Project>
